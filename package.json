{
  "name": "vscode-prisma-raw-query-sql",
  "displayName": "Prisma raw query SQL",
  "description": "Syntax highlighting for Prisma sql raw query",
  "version": "0.0.1",
  "publisher": "Akumzy",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Akumzy/vscode-sql-template-literal.git"
  },
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Languages"
  ],
  "contributes": {
    "grammars": [
      {
        "injectTo": [
          "source.js",
          "source.js.jsx",
          "source.jsx",
          "source.ts",
          "source.tsx"
        ],
        "scopeName": "inline.lit-sql",
        "path": "./syntaxes/lit-sql.json",
        "embeddedLanguages": {
          "meta.embedded.block.sql": "sql"
        }
      }
    ]
  }
}
